{
    "name": "warden-weather-test",
    "version": "1.0.0",
    "type": "commonjs",
    "scripts": {
        "dev:server": "ts-node-dev --respawn --transpile-only src/index.ts",
        "dev:frontend": "next dev",
        "dev": "concurrently \"npm run dev:server\" \"npm run dev:frontend\"",
        "build": "tsc && next build",
        "start:server": "node  dist/src/index.js",
        "start:frontend": "next start",
        "start": "concurrently \"npm run start:server\" \"npm run start:frontend\"",
        "prisma:gen": "prisma generate",
        "db:seed": "ts-node prisma/seed.ts"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.11.30",
        "@types/react": "19.1.10",
        "concurrently": "^9.2.0",
        "prisma": "^5.16.0",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.5.4"
    },
    "dependencies": {
        "@prisma/client": "^5.16.0",
        "@tailwindcss/postcss": "^4.1.12",
        "@tanstack/react-query": "^5.85.5",
        "axios": "^1.11.0",
        "cors": "^2.8.5",
        "cron": "^4.3.3",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "lucide-react": "^0.541.0",
        "next": "15.5.0",
        "next-themes": "^0.4.6",
        "postcss": "^8.5.6",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-hook-form": "^7.62.0",
        "react-icons": "^5.5.0",
        "tailwindcss": "^4.1.12"
    }
}
